<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <function name="write" model="ir.model.data">
            <function name="search" model="ir.model.data">
                <value
                  eval="[('module', '=', 'stride_payments'), ('name', '=', 'ir_cron_payment_transactions_batch')]"
                  />
            </function>
            <value eval="{'noupdate': False}" />
        </function>
        
        <record id="ir_cron_payment_transactions_batch" model="ir.cron">
           <field name="name">Import Payment Transactions Batch</field>
           <field name="model_id" ref="model_payment_transactions_batch"/>
           <field name="state">code</field>
           <field name="code">model.with_context(from_days=3,to_days=0).import_payment_transactions_batch()</field>
           <field name="user_id" ref="base.user_root"/>
           <field name='interval_number'>1</field>
           <field name='interval_type'>days</field>
           <field name="numbercall">-1</field>
           <field name="nextcall" eval="DateTime.now().replace(hour=22, minute=0)" />
        </record>
    </data>
</odoo>